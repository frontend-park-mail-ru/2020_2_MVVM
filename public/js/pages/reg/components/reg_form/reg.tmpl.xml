<fest:template xmlns:fest="http://fest.mail.ru" context_name="items">
    <div class="container">
        <form>
            <div class="reg">
                <form>
                    <div class="cfield">
                        <input id="nickReg" class="text" placeholder="Никнейм"/>
                        <span class="error" aria-live="polite"/>
                    </div>
                    <div class="cfield">
                        <input id="firstNameReg" class="text" placeholder="Имя"/>
                        <span class="error" aria-live="polite"/>
                    </div>
                    <div class="cfield">
                        <input id="lastNameReg" class="text" placeholder="Фамилия"/>
                        <span class="error" aria-live="polite"/>
                    </div>
                    <div class="cfield">
                        <input id="emailReg" class="text" placeholder="Email"/>
                        <span class="error" aria-live="polite"/>
                    </div>
                    <div class="cfield">
                        <input id="passwReg" class="password" type="password" placeholder="Введите пароль"/>
                        <span class="error" aria-live="polite"/>
                    </div>
                    <button class="enterBtn" type="submit">Зарегистрироваться</button>
                </form>
            </div>
        </form>
    </div>
</fest:template>