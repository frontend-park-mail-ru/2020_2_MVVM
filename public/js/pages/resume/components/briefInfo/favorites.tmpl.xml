<fest:template xmlns:fest="http://fest.mail.ru" context_name="items">
    <fest:if test="!items">
        <div class="download-cv" id="add_to_prefer"><i class="fas fa-heart download-cv__icon"></i>Добавить в избранное</div>
    </fest:if>
    <fest:if test="items">
        <div class="download-cv" id="delete_from_prefer"><i class="fas fa-eraser download-cv__icon"></i>Убрать из избранного</div>
    </fest:if>
</fest:template>